generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
  jobs  Job[]
}

model Job {
  id             Int     @id @default(autoincrement())
  inputType      String
  jobName        String  @unique
  createTime     String?
  status         String
  fileLocation   String?
  url            String?
  outputLocation String
  authorId       Int
  author         User    @relation(fields: [authorId], references: [id])
}
